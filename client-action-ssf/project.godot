[application]

config/name="rivernotch-game"
run/main_scene="res://ui/MainMenu.tscn"
config/icon="res://icon.png"

[autoload]

Constants="*res://globals/Constants.gd"
API="*res://globals/API.gd"
Util="*res://globals/Util.gd"
Events="*res://globals/Events.gd"
Console="*res://globals/Console.tscn"
WsEvents="*res://globals/WsEvents.gd"
Client="*res://globals/Client.tscn"
ErrorPanel="*res://globals/ErrorPanel.tscn"
